	compatible = "zmk,behavior-macro";
	label = "macro_email";
	#binding-cells = <0>;
	wait-ms = <30>
	tap-ms = <30>
	bindings = <&kp J>, <&kp E>, <&kp R>, <&kp E>, <&kp M>, <&kp Y>, 
		<&kp DOT>, <&kp L>, <&kp E>, <&kp R>, <&kp N>, <&kp E>, <&kp R>,
		<&kp AT>, <&kp R>, <&kp O>, <&kp C>, <&kp K>, <&kp S>, <&kp T>, <&kp A>, <&kp R>,
		<&kp N>, <&kp E>, <&kp W>, <&kp E>, <&kp N>, <&kp G>, <&kp L>, <&kp A>, <&kp N>, <&kp D>,
		<&kp DOT>, <&kp C>, <&kp O>, <&kp M>;
};
macro_cents: macro_cents{
	compatible = "zmk,behavior-macro";
	label = "macro_email";
	#binding-cells = <0>;
	wait-ms = <30>
	tap-ms = <30>
	bindings = <&macro_press &kp LALT>,
		<macro_tap &kp KP_N1 &kp KP_N5 &kp KB_N5>
		<&macro_release &kp LALT>,
};
