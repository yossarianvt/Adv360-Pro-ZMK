macro_email: macro_email{
	compatible = "zmk,behavior-macro";
	label = "macro_email";
	#binding-cells = <0>;
	wait-ms = <30>
	tap-ms = <30>
	bindings = <&kp L>, <&kp E>, <&kp R>, <&kp N>, <&kp E>, <&kp R>, <&kp J>, 
		<&kp AT>, <&kp G>, <&kp M>, <&kp A>, <&kp I>, <&kp L>, 
		<&kp DOT>, <&kp C>, <&kp O>, <&kp M>;
};
